<div id="journalContain">
	<form action="/submitJournal" method="post">
		<div class="journal container-md">
			<label for="entry" class="header form-label">
				<input class="title form-control" name="title" placeholder="title">
				<script> document.write(new Date().toDateString()); </script>
			</label>
			<div class="container">
				<div class="entry row">
					<div class="entryText" id="highlights"></div>
					<textarea class="entryText" name="entry" id="text"></textarea>
				</div>
			</div>
		</div>
		
		<button type="submit" class="journalBtn">submit</button>
	</form>
	<button id="sendDataBtn" class="journalBtn"><i class="fa-regular fa-clipboard"></i></button>


	<div id="responses"></div>
</div>