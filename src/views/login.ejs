<body>
	<div id="formContainer" class="container mb-3">
		<form id="loginForm" action="/login" method="post">
			
			<h6>Login to an Account</h6>
			
			<label for="email"></label>
			<input class="form-control form-control-sm" type="email" name="email" placeholder="Email">

			<label for="password"></label>
			<input class="form-control form-control-sm" type="password" name="password" placeholder="Password">

			<input id="submitBtn" class="btn btn-outline-secondary btn-sm" type="submit" value="Login">
			
		</form>
	</div> <!--formContainer-->
</body>